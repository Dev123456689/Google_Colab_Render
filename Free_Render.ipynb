        "# @title ##***Step 2: Download and Install Blender directly from blender.org (Alternative ðŸ‘‡)*** { display-mode: \"form\" }\n",
        "import os, shutil\n",
        "if 'Initialized' not in globals() :\n",
        "  import sys\n",
        "  sys.exit(\"Try again after completing Step 1: Initialize.\")\n",
        "elif Initialized == False or not os.path.exists(Work_path + '/Blender_Setup_Files') or not os.path.exists(Work_path +  '/Blend_File_in_Progress') or not os.path.exists(Work_path + '/Renders'):\n",
        "  Initialized = False\n",
        "  import sys\n",
        "  sys.exit(\"Try again after completing Step 1: Initialize.\")\n",
        "\n",
        "elif 'Installation_in_progress' in globals() == True and Installation_in_progress == True:\n",
        "  print(\"There was an error while performing the previous installation.\\nTrouble-shooting....\")\n",
        "  if os.path.isfile('/content/' + Selected_Blender_setup_filename):\n",
        "    shutil.rmtree('/content/' + Selected_Blender_setup_filename)\n",
        "  if os.path.isdir('/content/blender/'+Selected_Blender_setup_filename) == True or ('Blender_Version_Installed' in globals() == True and Installed_Blender_Version != None):\n",
        "    shutil.rmtree('/content/blender', ignore_errors=True)\n",
        "  print('Error Resolved.')\n",
        "\n",
        "from urllib import parse\n",
        "import requests, re\n",
        "import ipywidgets as widgets\n",
        "from bs4 import BeautifulSoup\n",
        "from IPython.display import display\n",
        "\n",
        "Save_downloaded_blender_setup_file_to_google_drive = False #@param {type:\"boolean\"}\n",
        "\n",
        "Installed_Blender_Version:str\n",
        "Installation_in_progress:bool\n",
        "\n",
        "\n",
        "# Create a session object\n",
        "session = requests.Session()\n",
        "\n",
        "col_manage = {\n",
        "  \"Filmic\": \"2.91\",\n",
        "  \"Agx\": \"4.0\"\n",
        "}\n",
        "\n",
        "def save_blender_setup_file_to_google_drive(setup_file_location:str):\n",
        "  shutil.copyfile(setup_file_location, Work_path + '/Blender_Setup_Files/' + setup_file_location.split('/')[-1])\n",
        "\n",
        "def Blender_Version_Dropdown_Creator():\n",
        "  global session\n",
        "  options_list = ['Select a Blender Version to download']\n",
        "  url = 'https://download.blender.org/release/'\n",
        "\n",
        "  # Use the session for requests\n",
        "  page = session.get(url)\n",
        "  soup = BeautifulSoup(page.content, 'lxml')\n",
        "\n",
        "  for link in reversed(soup.find_all('a')):\n",
        "    if re.match(r\"^Blender\\d+\\.\\d+/$\", link.text):\n",
        "      B_version = link.text.split('/')[0].split('Blender')[1]\n",
        "      if float(B_version) >= 2.60:\n",
        "        options_list.append(B_version)\n",
        "\n",
        "  blender_version_dropdown = widgets.Dropdown(\n",
        "      options = options_list,\n",
        "      value=options_list[0],\n",
        "      description='Blender Version:'\n",
        "  )\n",
        "\n",
        "  def dropdown_selection_handler(change):\n",
        "    selected_blender_version = 'None'\n",
        "\n",
        "    url2 = 'https://download.blender.org/release/Blender' + blender_version_dropdown.value + '/'\n",
        "    blender_version_dropdown.disabled = True\n",
        "\n",
        "    soup2 = BeautifulSoup(session.get(url2).content, 'lxml')\n",
        "    for link in reversed(soup2.find_all('a')):\n",
        "      if re.match(r\"blender-\\d\\.\\d+(\\.\\d+|([a-z]?|rc\\d))-linux(-glibc\\d+-x86_64|(-x)?64).tar.(bz2|xz)\", link.text):\n",
        "        filename = link.text\n",
        "        match = re.match(r\"blender-(\\d\\.\\d+)\", link.text)\n",
        "        if match:\n",
        "          selected_blender_version = match.group(1)\n",
        "        break\n",
        "\n",
        "    if selected_blender_version == 'None':\n",
        "      print('For this Blender version no suitable setup file was found')\n",
        "      sys.exit(\"Try again with another Blender version\")\n",
        "      return\n",
        "\n",
        "    setup_file_location = '/content/' + filename\n",
        "    # Selected_Blender_Version = version_selected\n",
        "    User_Consent_Manager(setup_file_location, selected_blender_version)\n",
        "    # Colormanagement_Dropdown_Creator(setup_file_location, version_selected)\n",
        "\n",
        "  blender_version_dropdown.observe(dropdown_selection_handler, names='value')\n",
        "  display(blender_version_dropdown)\n",
        "\n",
        "\n",
        "def disable_button(buttons:list[widgets.Button]):\n",
        "  for button in buttons:\n",
        "    button.disabled = True\n",
        "\n",
        "def User_Consent_Manager(setup_file_location:str, selected_blender_version:str):\n",
        "  if not os.path.exists('/content/blender'): # Check if Blender is installed\n",
        "    Download_Blender(setup_file_location, selected_blender_version)\n",
        "    return\n",
        "\n",
        "  print(\"\\nBlender \" + selected_blender_version + \" already exists.\")\n",
        "  if os.path.exists('/content/blender/' + setup_file_location.split('/')[-1] + '.txt'): # Check if the installed Blender version matches the selected version\n",
        "    print(f\"Do you want to reinstall Blender {selected_blender_version}?\")\n",
        "    buttons_displayed = []\n",
        "    yes_btn = widgets.Button(description='Yes, I want to reinstall it', layout=widgets.Layout(width='30%'))\n",
        "    buttons_displayed.append(yes_btn)\n",
        "    no_btn = widgets.Button(description=\"No, I don't want to reinstall it\", layout=widgets.Layout(width='30%'))\n",
        "    buttons_displayed.append(no_btn)\n",
        "    display(yes_btn)\n",
        "    display(no_btn)\n",
        "\n",
        "    yes_btn.on_click(lambda _: disable_button(buttons_displayed))\n",
        "    yes_btn.on_click(lambda _: Download_Blender(setup_file_location, selected_blender_version))\n",
        "    no_btn.on_click(lambda _: disable_button(buttons_displayed))\n",
        "    no_btn.on_click(lambda _: Complete_Cell_Execution())\n",
        "\n",
        "  else: # Ask the user if they want to replace the previously installed Blender\n",
        "    print(f\"Do you want to replace Blender {Installed_Blender_Version} with Blender {selected_blender_version}?\")\n",
        "    yes_btn = widgets.Button(description='Yes, I want to replace it', layout=widgets.Layout(width='30%'))\n",
        "    no_btn = widgets.Button(description=\"No, I don't want to replace it\", layout=widgets.Layout(width='30%'))\n",
        "    yes_btn.on_click(lambda _: disable_button([yes_btn, no_btn]))\n",
        "    yes_btn.on_click(lambda _: Download_Blender(setup_file_location, selected_blender_version))\n",
        "    no_btn.on_click(lambda _: disable_button([yes_btn, no_btn]))\n",
        "    no_btn.on_click(lambda _: Complete_Cell_Execution())\n",
        "    display(yes_btn)\n",
        "    display(no_btn)\n",
        "\n",
        "def Download_URL(url, filename: str = None, content: str = None):\n",
        "    def get_filename(url: str):\n",
        "      path = parse.unquote_plus(\n",
        "        parse.urlsplit(url).path, encoding=\"utf-8\", errors=\"replace\"\n",
        "      )\n",
        "      return path.split(\"/\")[-1]\n",
        "\n",
        "    r = requests.get(url, stream=True)\n",
        "    if r.status_code != 200:\n",
        "      print(f\"Requests Error {r.status_code}\")\n",
        "      SystemExit()\n",
        "    chunk_size = 786432\n",
        "    file_size = int(r.headers[\"content-length\"])\n",
        "    if chunk_size > file_size / 14:\n",
        "      chunk_size = int(file_size / 14)\n",
        "    if content is not None:\n",
        "      bar_format_start = f\"Downloading {content}:\"\n",
        "    else:\n",
        "      bar_format_start = \"Downloading:\"\n",
        "    bar_format = (\n",
        "        bar_format_start\n",
        "        + \"{l_bar}{bar}| {n_fmt}B / {total_fmt}B, {remaining} left, {rate_fmt}\"\n",
        "    )\n",
        "    bar = tqdm(\n",
        "        total=file_size,\n",
        "        unit=\"B\",\n",
        "        unit_divisor=1024,\n",
        "        unit_scale=True,\n",
        "        smoothing=0,\n",
        "        dynamic_ncols=True,\n",
        "        mininterval=0.15,\n",
        "        maxinterval=0.5,\n",
        "        bar_format=bar_format,\n",
        "    )\n",
        "    if filename is None:\n",
        "        filename = get_filename(url)\n",
        "    try:\n",
        "        with open(filename, \"wb\") as f:\n",
        "            for data in r.iter_content(chunk_size=chunk_size):\n",
        "                bar.update(len(data))\n",
        "                f.write(data)\n",
        "\n",
        "            bar.bar_format = (\n",
        "                bar_format.replace(\"remaining\", \"elapsed\")\n",
        "                .replace(\"left\", \"taken\")\n",
        "                .replace(\":\", \": \")\n",
        "                .replace(\"Downloading\", \"Downloaded\")\n",
        "                .replace(\"{n_fmt}B / \", \"\")\n",
        "            )\n",
        "            bar.close()\n",
        "    except:\n",
        "        if os.path.exists(filename):\n",
        "            os.remove(filename)\n",
        "        SystemExit()\n",
        "\n",
        "def Download_Blender(setup_file_location:str, selected_blender_version:str):\n",
        "  global Installation_in_progress\n",
        "  Installation_in_progress = True\n",
        "\n",
        "  # if file already exists and user wants to save it to google drive\n",
        "  # then delete it so it can be replaced with a fresh copy\n",
        "  # else install the blender version\n",
        "  if os.path.exists(setup_file_location):\n",
        "    if Save_downloaded_blender_setup_file_to_google_drive == True:\n",
        "      os.remove(setup_file_location)\n",
        "    else:\n",
        "      Install_Blender(setup_file_location, selected_blender_version)\n",
        "      return\n",
        "\n",
        "  filename = setup_file_location.split('/')[-1]\n",
        "  url = 'https://download.blender.org/release/Blender' + selected_blender_version + '/' + filename\n",
        "#   !wget -q --show-progress $url\n",
        "  Download_URL(url, content=\"Blender\")\n",
        "  if Save_downloaded_blender_setup_file_to_google_drive == True:\n",
        "    save_blender_setup_file_to_google_drive(setup_file_location)\n",
        "  Install_Blender(setup_file_location, selected_blender_version)\n",
        "\n",
        "def Install_Blender(setup_file_location:str, selected_blender_version:str):\n",
        "  global Installation_in_progress\n",
        "  Installation_in_progress = True\n",
        "  global Installed_Blender_Version\n",
        "\n",
        "  print('\\n')\n",
        "\n",
        "  if os.path.exists('/content/blender') == True:\n",
        "    Installed_Blender_Version = None\n",
        "    shutil.rmtree('/content/blender/')\n",
        "    print('Uninstalled Old Blender installation.')\n",
        "\n",
        "  print('Installing requirements...')\n",
        "  os.environ[\"LD_PRELOAD\"] = \"\"\n",
        "\n",
        "  !sudo DEBIAN_FRONTEND=noninteractive apt-get -yq -o Dpkg::Options::=\"--force-confdef\" -o Dpkg::Options::=\"--force-confold\" remove libtcmalloc-minimal4 2>/dev/null > /dev/null\n",
        "\n",
        "  !sudo DEBIAN_FRONTEND=noninteractive apt-get -yq -o Dpkg::Options::=\"--force-confdef\" -o Dpkg::Options::=\"--force-confold\" install google-perftools 2>/dev/null > /dev/null\n",
        "  !sudo DEBIAN_FRONTEND=noninteractive apt-get -yq -o Dpkg::Options::=\"--force-confdef\" -o Dpkg::Options::=\"--force-confold\" install libgoogle-perftools4 2>/dev/null > /dev/null\n",
        "  !sudo DEBIAN_FRONTEND=noninteractive apt-get -yq -o Dpkg::Options::=\"--force-confdef\" -o Dpkg::Options::=\"--force-confold\" install libtcmalloc-minimal4 2>/dev/null > /dev/null\n",
        "\n",
        "  os.environ[\"LD_PRELOAD\"] = \"/usr/lib/x86_64-linux-gnu/libtcmalloc_minimal.so.4.5.9\"\n",
        "\n",
        "  print(\"Installing Blender please wait patiently, it may take upto a minute\")\n",
        "  os.mkdir('/content/blender')\n",
        "  !tar xf $setup_file_location -C ./blender --strip-components 1\n",
        "  open(\"/content/blender/\"+ setup_file_location.split('/')[-1] + \".txt\", \"x\").close()\n",
        "  Installed_Blender_Version = selected_blender_version\n",
        "\n",
        "  print('Blender Installed')\n",
        "\n",
        "  Complete_Cell_Execution()\n",
        "\n",
        "\n",
        "def Complete_Cell_Execution():\n",
        "  global Installation_in_progress\n",
        "  Installation_in_progress = False\n",
        "  print('\\nEverything is ready to go, have fun!')\n",
        "\n",
        "\n",
        "Blender_Version_Dropdown_Creator()"
      ],
