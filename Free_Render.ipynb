        "#@title ##**GPU & Ram  Information â„¹** { vertical-output: true, display-mode: \"form\" }\n",
        "! nvidia-smi\n",
        "import psutil\n",
        "def get_size(bytes, suffix=\"B\"):\n",
        "    factor = 1024\n",
        "    for unit in [\"\", \"K\", \"M\", \"G\", \"T\", \"P\"]:\n",
        "        if bytes < factor:\n",
        "            return f\"{bytes:.2f}{unit}{suffix}\"\n",
        "        bytes /= factor\n",
        "print(\"\\n+\" + \"-\"*30 + \"Memory Information\" + \"-\"*29 + \"+\")\n",
        "svmem = psutil.virtual_memory()\n",
        "print(\"| Total: \" + get_size(svmem.total)+\" \"*62 + \"|\") ; print(\"| Available: \"+get_size(svmem.available)+\" \"*58+\"|\")\n",
        "print(\"| Used: \"+get_size(svmem.used)+\" \"*62 + \"|\") ; print(f\"| Percentage: {svmem.percent}%\" + \" \"*60 + \"|\")\n",
        "print(\"+\" + \"-\"*77 +\"+\")"
      ],
